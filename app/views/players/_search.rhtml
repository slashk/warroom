<table cellpadding="0" cellspacing="0" border="0" id="players"><thead><tr><th class="l">Player</th><th class="c">Watch</th><th class="c">Pos</th> <th class="c">Team</th><th class="c">Rank</th><th class="c">O-Rank</th><th class="c">Depth</th><th>R</th><th>HR</th><th>RBI</th><th>SB</th><th>AVG</th> <th>IP</th><th>W</th><th>SV</th><th>K</th><th>ERA</th><th>WHIP</th></tr></thead><tbody>
<% unless @players.nil? -%>
<% for player in @players -%>
<tr id="player-<%= player.id -%>"><td class="l"><a href="http://sports.yahoo.com/mlb/players/<%=h player.yahoo_ref -%>" target="_blank"> <%= player.player -%></a><%= content_tag :span, player.status, :class => "status" if player.status? -%></td><td class="c"><%= render(:partial => @watchlist.include?(player.id) ? "watched" : "unwatched", :locals => {:player => player}) -%></td><td class="c"><%=h player.pos -%></td><td class="c"><%=h player.team.upcase -%></td><td class="c"><%=h player.rank -%></td><td class="c"><%=h player.orank -%></td> <td class="c"><%=h player.depth -%></td> <td><%=h player.R -%></td><td><%=h player.HR %></td> <td><%=h player.RBI %></td><td><%=h player.SB %></td> <td><%= ba(player.AVG) if (player.AVG!=nil) %></td><td><%=h player.IP %></td><td><%=h player.W %></td> <td><%=h player.SV %></td><td><%=h player.K %></td><td><%=h sprintf("%.2f",player.ERA) if (player.ERA!=nil) %></td><td><%=h sprintf("%.2f",player.WHIP) if (player.WHIP!=nil) %></td></tr><% end -%><% end -%></tbody></table>