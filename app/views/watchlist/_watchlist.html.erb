<% unless @watchlist.nil? -%>
	<span class="watchlistPlayer"><%= watchlist.player.player %> (<%= watchlist.player.pos %>) <%= content_tag :span, watchlist.player.status, :class => "status" if watchlist.player.status? -%>
	<% if @my_pick -%> 
		<%=  link_to image_tag("user_add.png", :size => "16x16", :id => watchlist.player.id, :class => "watchlistPlayerIcon", :border => 0), "#" %>
	<% end -%>
	</span><br>
<% end -%>